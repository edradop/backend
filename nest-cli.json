{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/edradop/src",
  "compilerOptions": {
    "typeCheck": true,
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/edradop/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/edradop",
  "projects": {
    "edradop": {
      "type": "application",
      "root": "apps/edradop",
      "entryFile": "main",
      "sourceRoot": "apps/edradop/src",
      "compilerOptions": {
        "tsConfigPath": "apps/edradop/tsconfig.app.json"
      }
    },
    "authentication": {
      "type": "application",
      "root": "apps/authentication",
      "entryFile": "main",
      "sourceRoot": "apps/authentication/src",
      "compilerOptions": {
        "tsConfigPath": "apps/authentication/tsconfig.app.json"
      }
    },
    "storage": {
      "type": "application",
      "root": "apps/storage",
      "entryFile": "main",
      "sourceRoot": "apps/storage/src",
      "compilerOptions": {
        "tsConfigPath": "apps/storage/tsconfig.app.json"
      }
    },
    "analytics": {
      "type": "application",
      "root": "apps/analytics",
      "entryFile": "main",
      "sourceRoot": "apps/analytics/src",
      "compilerOptions": {
        "tsConfigPath": "apps/analytics/tsconfig.app.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    },
    "payment": {
      "type": "application",
      "root": "apps/payment",
      "entryFile": "main",
      "sourceRoot": "apps/payment/src",
      "compilerOptions": {
        "tsConfigPath": "apps/payment/tsconfig.app.json"
      }
    },
    "communication": {
      "type": "application",
      "root": "apps/communication",
      "entryFile": "main",
      "sourceRoot": "apps/communication/src",
      "compilerOptions": {
        "tsConfigPath": "apps/communication/tsconfig.app.json"
      }
    },
    "analytic": {
      "type": "application",
      "root": "apps/analytic",
      "entryFile": "main",
      "sourceRoot": "apps/analytic/src",
      "compilerOptions": {
        "tsConfigPath": "apps/analytic/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}